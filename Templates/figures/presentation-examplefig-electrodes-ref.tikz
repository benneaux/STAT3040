            \draw [name path global=r3]
                let \p1=($(o2)-(o2 |- cz)$),
                    \p2=($(c6)-(o2 |- cz)$),
                    \n1={\y1/\x2*\y1/2+\x2/2}
                in
                    ($(c6)-(\n1,0)$) circle (\n1);
        \end{scope}
