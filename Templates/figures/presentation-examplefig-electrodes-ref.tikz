    \node (o2) [el] at (c.288) {\el{O}{2}};
    \node (po8) [el] at (c.306) {\el{PO}{8}};
    \node (p8) [el] at (c.324) {\el{P}{8}};
    \node (tp8) [el] at (c.342) {\el{TP}{8}};
