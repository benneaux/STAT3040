    \node (f6) [el,name intersections={of=r3 and u2,by=x}] at (x) {\el{F}{6}};
    \node (fc6) [el,name intersections={of=r3 and u1,by=x}] at (x) {\el{FC}{6}};
    \node (cp6) [el,name intersections={of=r3 and d1,by=x,sort by=d1}] at (x) {\el{CP}{6}};
