            \draw [name path global=u1]
                let \p1=($(ft7)-(ft7 -| cz)$),
                    \p2=($(ft7 -| cz)-(fcz)$),
                    \n1={\x1/\y2*\x1/2+\y2/2}
                in
                    ($(fcz)+(0,\n1)$) circle (\n1);
            \draw [name path global=u2]
                let \p1=($(f7)-(f7 -| cz)$),
                    \p2=($(f7 -| cz)-(fz)$),
                    \n1={\x1/\y2*\x1/2+\y2/2}
                in
                    ($(fz)+(0,\n1)$) circle (\n1);
            \draw [name path global=u3]
                let \p1=($(af7)-(af7 -| cz)$),
                    \p2=($(af7 -| cz)-(afz)$),
                    \n1={\x1/\y2*\x1/2+\y2/2}
                in
                    ($(afz)+(0,\n1)$) circle (\n1);
            \draw [name path global=l1]
                let \p1=($(oz)-(cz)$),
                    \p2=($(cz)-(c1)$),
                    \n1={\y1/\x2*\y1/2+\x2/2}
                in
                    ($(c1)+(\n1,0)$) circle (\n1);
            \draw [name path global=l2]
                let \p1=($(o1)-(o1 |- cz)$),
                    \p2=($(o1 |- cz)-(c3)$),
                    \n1={\y1/\x2*\y1/2+\x2/2}
                in
                    ($(c3)+(\n1,0)$) circle (\n1);
            \draw [name path global=l3]
                let \p1=($(o1)-(o1 |- cz)$),
