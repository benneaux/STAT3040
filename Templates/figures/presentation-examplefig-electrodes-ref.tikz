    \coordinate (back) at ($(nose)!2!(cz)$);
    \node (t7) [el] at ($(leftear)!.2!(cz)$) {\el{T}{7}};
    \node (c5) [el] at ($(leftear)!.4!(cz)$) {\el{C}{5}};
    \node (c3) [el] at ($(leftear)!.6!(cz)$) {\el{C}{3}};
    \node (c1) [el] at ($(leftear)!.8!(cz)$) {\el{C}{1}};
